{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Alif Ensemble Debug",
            "cwd": "${workspaceFolder}",
            "executable": "./out/cproject/${command:cpptools.activeConfigName}/cproject.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "jlink",
            "showDevDebugOutput": "parsed",
            "device": "AE722F80F55D5_${command:cpptools.activeConfigName}",
            "interface": "jtag",
            "preLaunchCommands": [
                "monitor speed 15000"
            ],
            "postLaunchCommands": [
                "set $sp = __Vectors[0]",
                "set $pc = __Vectors[1]",
                "set *0xe000ed08 = __Vectors"
            ]
        }
    ]
}